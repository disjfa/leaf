{
    "type": "project",
    "license": "proprietary",
    "homepage": "https://github.com/disjfa/glynn-admin-symfony4",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/disjfa/timetable-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/disjfa/media-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/disjfa/maps-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/disjfa/pwa-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/disjfa/FullCalendarBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/disjfa/event-bundle"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "ext-json": "*",
        "cache/filesystem-adapter": "^1.0",
        "disjfa/enqueue-bundle": "dev-master",
        "disjfa/event-bundle": "dev-master",
        "disjfa/glynn-theme-bundle": "dev-master",
        "disjfa/maps-bundle": "dev-master",
        "disjfa/media-bundle": "dev-master",
        "disjfa/menu-bundle": "dev-master",
        "disjfa/mozaic-bundle": "dev-master",
        "disjfa/pwa-bundle": "dev-master",
        "disjfa/timetable-bundle": "dev-master",
        "disjfa/translation-bundle": "dev-master",
        "easycorp/easyadmin-bundle": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.10",
        "friendsofsymfony/user-bundle": "~2.0",
        "fzaninotto/faker": "^1.8",
        "guzzlehttp/guzzle": "^6.3",
        "knplabs/knp-menu-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^2.8",
        "liip/imagine-bundle": "^2.1",
        "ornicar/gravatar-bundle": "^1.2",
        "php-translation/common": "^1.0",
        "php-translation/symfony-bundle": "^0.8.0",
        "php-translation/symfony-storage": "^1.0",
        "phpmob/settings-bundle": "^1.2",
        "ramsey/uuid": "^3.8",
        "samj/fractal-bundle": "^2.1",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/mime": "^4.3@dev",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symfony/web-server-bundle": "^4.0",
        "symfony/webpack-encore-bundle": "^1.2",
        "symfony/workflow": "^4.0",
        "symfony/yaml": "^4.0",
        "toiba/fullcalendar-bundle": "dev-master",
        "twig/cssinliner-extension": "^2.0",
        "twig/inky-extension": "^2.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist",
            "disjfa/*": "source"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C3XTTD63DTHK3Y1E6YEZBAH8",
            "allow-contrib": true
        }
    }
}
